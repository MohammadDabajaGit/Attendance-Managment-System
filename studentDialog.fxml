          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
             <TextField fx:id="idInsertField" promptText="student id" GridPane.columnIndex="0" visible="false"/>
            <TextField fx:id="nameInsertField" promptText="student name" GridPane.columnIndex="1" />
            <TextField fx:id="passInsertField" promptText="student pass" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </content>
