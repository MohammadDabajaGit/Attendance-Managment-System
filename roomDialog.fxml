<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="220.0" prefWidth="351.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RoomController">
    <content>
        <GridPane hgap="10.0" prefHeight="63.0" prefWidth="438.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="nameInsertField" promptText="room name" GridPane.columnIndex="0"/>
                <Spinner fx:id="capacityInsertField" promptText="room Capacity" GridPane.columnIndex="1" min="0" max="1000" editable="true" initialValue="50"/>
            </children>
        </GridPane>
    </content>
</DialogPane>
