<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="220.0" prefWidth="351.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SessionController">
    <content>
        <GridPane hgap="10.0" prefHeight="63.0" prefWidth="438.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="nameInsertField" promptText="session name" GridPane.columnIndex="0" />
                <TextField fx:id="courseInsertField" promptText="course id" GridPane.columnIndex="1" />
                <TextField fx:id="teacherInsertField" promptText="teacher id" GridPane.columnIndex="2" />
                <TextField fx:id="roomInsertField" promptText="room id" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <TextArea fx:id="descriptionInsertField" promptText="session description" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.rowSpan="3" wrapText="true" minWidth="310"/>
                <DatePicker fx:id="timeInsertField" promptText="session time" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            </children>
        </GridPane>
    </content>
</DialogPane>